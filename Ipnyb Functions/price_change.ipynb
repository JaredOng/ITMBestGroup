    "import json\n",
    "def price_change(product,value):\n",
    "    f = open(\"Product_List.json\")\n",
    "    pdb = json.load(f)\n",
    "\n",
    "    pdb[product] = value\n",
    "\n",
    "    with open('Product_List.json',\"w\")as d:\n",
    "        json.dump(pdb,d,indent=4)\n",
    "\n",
    "price_change(\"Bullhead Milk\",2700)"
   ],
